for numero in range(1, 11):
    if numero == 5:
        print("Se encontró el número 5, deteniendo el bucle...")
        break
    print("Número:", numero)

print("Fin del bucle con break")

#EJEMPLO 2

for numero in range(1, 6):
    if numero == 3:
        print("Saltando el número 3...")
        continue
    print("Número:", numero)

print("Fin del bucle con continue")
